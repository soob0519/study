<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="donghwa.mapper.AdminfoMapper">

	<insert id="insertAdminfo">
		INSERT INTO ADMINFO (
							USERID
							,USERPASS
							,USERNAME
							,BIRTH
							,GRADE
							,STATE
							,RDATE ) 
					VALUES(
							'${userid}'
							,'${userpass}'
							,'${username}'
							,'${birth}'
							,'${grade}'
							,'${state}'
							,SYSDATE )
	</insert>
</mapper>